{
	"name": "csdesigner-plugin",
	"displayName": "csdesigner-plugin",
	"description": "Associate .ui files to start Copperspice Designer",
	"publisher": "Copperspice",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:csdesigner.start",
		"onCommand:csdesigner.search"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "csdesigner.start",
				"title": "Open in CopperSpice Designer"
			},
			{
				"command": "csdesigner.search",
				"title": "Search in CopperSpice Documentation"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname == .ui",
					"command": "csdesigner.start",
					"group": "navigation"
				}
			],
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "csdesigner.search"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"keywords": [
		"vscode",
		"open",
		"copperspice",
		"external",
		"designer",
		"ui"
	],
	"bugs": {
		"url": "https://github.com/janwilmans/"
	},
	"author": {
		"name": "Jan Wilmans",
		"email": "janwilmans@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/janwilmans/"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}